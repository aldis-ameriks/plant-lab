{
  "name": "client",
  "version": "1.0.0",
  "license": "GPL-3.0",
  "dependencies": {
    "@apollo/client": "3.2.3",
    "apexcharts": "3.22.0",
    "date-fns": "2.16.1",
    "graphql": "15.3.0",
    "react": "16.13.1",
    "react-apexcharts": "1.3.7",
    "react-dom": "16.13.1",
    "react-ga": "3.1.2",
    "react-router-dom": "5.2.0",
    "styled-components": "5.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.10",
    "@graphql-codegen/typescript": "1.17.10",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@graphql-codegen/typescript-react-apollo": "2.0.7",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.0.4",
    "@types/jest": "26.0.14",
    "@types/node": "14.11.8",
    "@types/react": "16.9.51",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.6",
    "@types/styled-components": "5.1.4",
    "@types/testing-library__react": "10.0.1",
    "@typescript-eslint/eslint-plugin": "4.4.0",
    "@typescript-eslint/parser": "4.4.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-typescript": "11.0.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.21.4",
    "prettier": "2.1.2",
    "react-scripts": "3.4.3",
    "typescript": "3.9.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .ts,.tsx --fix --ignore-path .gitignore",
    "generate": "gql-gen && eslint --fix src/graphql-gen.tsx && prettier --write src/graphql-gen.tsx",
    "docker:build": "docker build -t registry.kataldi.com/plant-client .",
    "docker:push": "docker push registry.kataldi.com/plant-client",
    "docker:build:push": "yarn docker:build && yarn docker:push",
    "docker:run": "docker run -p 80:80 --rm registry.kataldi.com/plant-client"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 120
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
