    "expo-font": "~11.4.0",
    "expo-linking": "~5.0.2",
    "expo-router": "^2.0.0",
    "expo-secure-store": "~12.3.1",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "graphql": "15.8.0",
    "react": "18.2.0",
    "react-native": "0.72.10",
    "react-native-chart-kit": "6.12.0",
