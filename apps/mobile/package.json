    "expo-font": "~11.10.3",
    "expo-linking": "~6.2.2",
    "expo-router": "^3.4.8",
    "expo-secure-store": "~12.8.1",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "graphql": "16.8.1",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-chart-kit": "6.12.0",
