{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .ts,.tsx --ignore-path .gitignore --fix",
    "format": "prettier --write --ignore-path .gitignore .",
    "test": "jest",
    "coverage": "jest --coverage",
    "generate": "gql-gen -r dotenv/config && eslint --fix src/graphql/graphql-gen.tsx && prettier --write src/graphql/graphql-gen.tsx",
    "docker:build": "docker build -t registry.kataldi.com/pm-web .",
    "docker:build:mac": "docker buildx build --platform linux/amd64 --push -t registry.kataldi.com/pm-web .",
    "docker:push": "docker push registry.kataldi.com/pm-web",
    "docker:build:push": "npm run docker:build && npm run docker:push",
    "docker:run": "docker run -p 3000:3000 --name pm-web --rm registry.kataldi.com/pm-web"
  },
  "dependencies": {
    "@urql/exchange-retry": "1.0.0",
    "date-fns": "2.29.3",
    "graphql": "16.6.0",
    "graphql-tag": "2.12.6",
    "next": "13.1.1",
    "polished": "4.2.2",
    "react": "18.2.0",
    "react-apexcharts": "1.3.7",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "styled-components": "5.3.6",
    "urql": "3.0.3"
  },
  "devDependencies": {
    "@graphql-codegen/add": "3.2.3",
    "@graphql-codegen/cli": "2.16.1",
    "@graphql-codegen/typescript": "2.8.5",
    "@graphql-codegen/typescript-operations": "2.5.10",
    "@graphql-codegen/typescript-urql": "3.7.3",
    "@next/bundle-analyzer": "13.1.1",
    "@testing-library/dom": "8.19.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.47.0",
    "@typescript-eslint/parser": "5.47.0",
    "babel-jest": "29.3.1",
    "babel-plugin-macros": "3.1.0",
    "babel-plugin-styled-components": "2.0.7",
    "cross-fetch": "3.1.5",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "msw": "0.49.2",
    "prettier": "2.8.1",
    "typescript": "4.9.4",
    "webpack": "5.75.0"
  }
}
